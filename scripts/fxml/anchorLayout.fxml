<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0"
	prefWidth="700.0" stylesheets="style.css" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxml.Controller">
	<children>
		<HBox layoutX="25" layoutY="10.0" spacing="10.0"
			AnchorPane.leftAnchor="15.0">
			<children>
				<Label text="Search">
				</Label>
				<TextField id="textField1" fx:id="searchText" layoutX="60"
					layoutY="10.0" />
			</children>
		</HBox>
		<TableView fx:id="table" layoutX="25.0" layoutY="44.0"
			prefHeight="200.0" prefWidth="400.0" AnchorPane.bottomAnchor="15.0"
			AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0"
			AnchorPane.topAnchor="44.0">
			<columns>
				<TableColumn fx:id="titleCol" minWidth="400.0" text="Title" />
				<TableColumn fx:id="stockCol" maxWidth="100.0" minWidth="100.0"
					text="Stock" />
				<TableColumn fx:id="yearCol" maxWidth="100.0" minWidth="100.0"
					text="Year" />
			</columns>
			<columnResizePolicy>
				<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
			</columnResizePolicy>
		</TableView>
	</children>
</AnchorPane>
