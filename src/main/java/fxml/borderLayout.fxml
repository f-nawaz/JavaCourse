<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="360.0" prefWidth="681.0" stylesheets="fxml.style.css"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
	fx:controller="fxml.Controller">
	<top>
		<HBox prefHeight="38.0" prefWidth="641.0" spacing="10.0"
			BorderPane.alignment="CENTER">
			<children>
				<Label text="Search" />
				<TextField id="textField1" fx:id="searchText" />
			</children>
			<BorderPane.margin>
				<Insets />
			</BorderPane.margin>
		</HBox>
	</top>
	<center>
		<TableView fx:id="table" prefHeight="200.0" prefWidth="200.0"
			BorderPane.alignment="CENTER">
			<columns>
				<TableColumn fx:id="titleCol" minWidth="400.0"
					prefWidth="75.0" text="Title" />
				<TableColumn fx:id="stockCol" minWidth="100.0"
					prefWidth="75.0" text="Stock" />
				<TableColumn fx:id="yearCol" minWidth="100.0" prefWidth="75.0"
					text="Year" />
			</columns>
		</TableView>
	</center>
	<padding>
		<Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
	</padding>
</BorderPane>
